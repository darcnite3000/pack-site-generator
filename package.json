{
  "name": "pack-site-generator",
  "version": "1.0.0",
  "description": "site generator that builds from the pack system",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server build:watch",
    "prebuild:watch": "npm run clean",
    "build:watch": "webpack --env.dev --watch",
    "prebuild": "npm run clean",
    "build": "webpack --env.prod -p",
    "clean": "find -E www -type f ! -regex \".*\\.(php|htaccess|json)\" -delete",
    "server": "php -S localhost:8000 -t www/ router.php"
  },
  "author": "Tristan Smith <darcnite3000@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-jsx": "^2.0.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "node-sass": "^3.8.0",
    "npm-run-all": "^3.0.0",
    "postcss-loader": "^0.10.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.21",
    "webpack-config-utils": "^2.0.0",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2"
  }
}
